<%= provide(:title, 'Magazine Form') %>
<h1>Magazine Form</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@magazine) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>
	  <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :issue %>
      <%= f.text_field :issue %>
	  
	  <%= f.submit "Create the magazine", class: "btn btn-large btn-primary" %>
	<% end %>
  </div>
</div>
